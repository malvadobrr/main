<script type='text/javascript'>
      function getUrlVars() {
        var vars = {};
        var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function (m, key, value) {
        vars[key] = value;
      });
          return vars;
      }
      var link = getUrlVars()["src"];
    </script>
